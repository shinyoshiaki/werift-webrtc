{
  "name": "browser",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.server.json",
    "ci": "yarn build && run-p prod:server prod:test",
    "debug": "cd debug && yarn dev",
    "type": "tsc --noEmit -p tsconfig.server.json && tsc --noEmit -p tsconfig.json",
    "dev:server": "DEBUG=werift* ts-node-dev --project tsconfig.server.json ./server/main.ts",
    "dev:chrome": "karma start --browsers chrome_with_fake_device",
    "dev:firefox": "karma start --browsers firefox_with_fake_device",
    "prod:server": "DEBUG=werift* node lib/e2e/server/main.js",
    "prod:test": "karma start --single-run --browsers chrome_headless_with_fake_device && node stop.js"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "werift": "^0.13.7",
    "werift-rtp": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.8.1",
    "@types/node": "^16.3.3",
    "@types/protoo-client": "^4.0.1",
    "@types/protoo-server": "^4.0.2",
    "axios": "^0.21.2",
    "babel-preset-env": "^1.7.0",
    "express": "^4.17.1",
    "jasmine-core": "^3.8.0",
    "jsonc-parser": "^3.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.1",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.2",
    "karma-typescript-es6-transform": "^5.5.2",
    "npm-run-all": "^4.1.5",
    "protoo-client": "^4.0.6",
    "protoo-server": "^4.0.6",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
