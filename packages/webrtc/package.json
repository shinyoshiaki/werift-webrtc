{
  "name": "werift",
  "version": "0.0.40",
  "description": "WebRTC Implementation for TypeScript (Node.js)",
  "keywords": [
    "WebRTC",
    "node.js"
  ],
  "homepage": "https://github.com/shinyoshiaki/rainy-webrtc",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinyoshiaki/rainy-webrtc.git"
  },
  "license": "MIT",
  "author": {
    "name": "shinyoshiaki"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rm -rf lib && yarn format && rollup -c",
    "depcheck": "depcheck --ignores='@typescript-eslint/*,eslint-*'",
    "doc": "rm -rf doc && typedoc --mode file --out doc ./src/**/*.ts",
    "format": "yarn unused && yarn lint && prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "test": "npm run type && jest --runInBand --forceExit",
    "type": "tsc --noEmit --project ./tsconfig.json",
    "unused": "organize-imports-cli src/**/*.ts"
  },
  "dependencies": {
    "@fidm/x509": "^1.2.1",
    "aes-js": "^3.1.2",
    "big-integer": "^1.6.48",
    "binary-data": "^0.6.0",
    "buffer-crc32": "^0.2.13",
    "debug": "^4.3.1",
    "elliptic": "^6.5.3",
    "int64-buffer": "^1.0.0",
    "ip": "^1.1.5",
    "jspack": "^0.0.4",
    "lodash": "^4.17.20",
    "p-cancelable": "^2.0.0",
    "rx.mini": "^1.0.1",
    "turbo-crc32": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@types/aes-js": "^3.1.1",
    "@types/big-integer": "^0.0.31",
    "@types/buffer-crc32": "^0.2.0",
    "@types/debug": "^4.1.5",
    "@types/elliptic": "^6.4.12",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.166",
    "@types/uuid": "^8.3.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "cypress": "^6.2.0",
    "python-shell": "^2.0.3",
    "rollup": "^2.35.1",
    "typedoc": "^0.20.5"
  },
  "engines": {
    "node": ">=14"
  },
  "readme": "README.md"
}
